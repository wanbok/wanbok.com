<p id="notice"><%= notice %></p>

<p>
  <b>User:</b>
  <%= @survey.user %>
</p>

<p>
  <b>분류:</b>
  <%= @aggregation[:class] %>
</p>

<p>
  <b>총점수:</b>
  <%= @aggregation[:total] %>
</p>

<p>
  <b>Title:</b>
  <%= @survey.title %>
</p>

<p>
  <b>Sex:</b>
  <%= @survey.sex %>
</p>

<p>
  <b>Age:</b>
  <%= @survey.age %>
</p>

<% (1..15).each do |q| %>
  <p>
    <b>Q<%= q %>:</b>
    <%= @survey["q#{q}".to_sym] %>
  </p>
<% end %>

